{
  "extends": ["next/core-web-vitals", "airbnb-base"],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react", "react-hooks"],
  "rules": {
    "react/prop-types": "off",
    "react/display-name": "off",
    "quotes": ["error", "double"]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
